{
    "manifest_version": 2,
    "name": "Speechify",
    "version": "0.0.2",
    "permissions": [
        "tts",
        "contextMenus"
    ],
    "browser_action": {
        "default_title": "Speechify"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    }
}
