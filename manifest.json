{
    "manifest_version": 2,
    "name": "Chrome TTS Test",
    "version": "0.0.1",
    "permissions": [
        "tts",
        "contextMenus"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    }
}
